<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sea.dao.EquipmenbatteryProbeSer">
	<resultMap id="equipmenbatteryProbeMapResult" type="com.ctevs.po.EquipmenbatteryProbeEntity">
		<result column="id" property="id" /> <!-- 序列 -->
		<result column="equipment_id" property="equipmentId" /> <!-- 设备ID 12位 -->
		<result column="batch_no" property="batchNo" /> <!-- 设备数据批次号 -->
		<result column="battery_sys_no" property="batterySysNo" /> <!-- 可充电储能子系统编号 -->
		<result column="probe_number" property="probeNumber" /> <!-- 可充电储能温度探针个数（Nt） -->
		<result column="one_probe_no" property="oneProbeNo" /> <!-- 单体探针编号 -->
		<result column="one_probe_temperature" property="oneProbeTemperature" /> <!-- 单体探针温度 -->
		<result column="collection_time" property="collectionTime" /> <!-- 信息采集时间 -->
		<result column="addtime" property="addtime" /> <!-- 创建时间 -->
	</resultMap>

	<sql id="select-all-column">
		id ,
		equipment_id ,
		batch_no ,
		battery_sys_no ,
		probe_number ,
		one_probe_no ,
		one_probe_temperature ,
		collection_time ,
		addtime
	</sql>

	<sql id="where-all-page-condition">
		<if test="f.id != null">
			AND id = #{f.id}
		</if>
		<if test="f.equipmentId != null and f.equipmentId != ''">
			AND equipment_id = #{f.equipmentId}
		</if>
		<if test="f.batchNo != null">
			AND batch_no = #{f.batchNo}
		</if>
		<if test="f.batterySysNo != null">
			AND battery_sys_no = #{f.batterySysNo}
		</if>
		<if test="f.probeNumber != null">
			AND probe_number = #{f.probeNumber}
		</if>
		<if test="f.oneProbeNo != null">
			AND one_probe_no = #{f.oneProbeNo}
		</if>
		<if test="f.oneProbeTemperature != null">
			AND one_probe_temperature = #{f.oneProbeTemperature}
		</if>
		<if test="f.collectionTime != null">
			AND collection_time = #{f.collectionTime}
		</if>
		<if test="f.addtime != null">
			AND addtime = #{f.addtime}
		</if>
	</sql>

	<sql id="where-all-condition">
		<if test="id != null">
			AND id = #{id}
		</if>
		<if test="equipmentId != null and equipmentId != ''">
			AND equipment_id = #{equipmentId}
		</if>
		<if test="batchNo != null">
			AND batch_no = #{batchNo}
		</if>
		<if test="batterySysNo != null">
			AND battery_sys_no = #{batterySysNo}
		</if>
		<if test="probeNumber != null">
			AND probe_number = #{probeNumber}
		</if>
		<if test="oneProbeNo != null">
			AND one_probe_no = #{oneProbeNo}
		</if>
		<if test="oneProbeTemperature != null">
			AND one_probe_temperature = #{oneProbeTemperature}
		</if>
		<if test="collectionTime != null">
			AND collection_time = #{collectionTime}
		</if>
		<if test="addtime != null">
			AND addtime = #{addtime}
		</if>
	</sql>

	<!-- query list with page -->
	<select id="selectEquipmenbatteryProbeListByPageCond" resultMap="equipmenbatteryProbeMapResult">
		SELECT
		<include refid="select-all-column" />
		FROM T_EQUIPMENT_BATTERY_PROBE WHERE 1=1
		<include refid="where-all-page-condition" />
		LIMIT
		#{beginCount},#{pageRows}
	</select>

	<!-- query list count -->
	<select id="selectEquipmenbatteryProbeListTotalCount" resultType="int">
		SELECT COUNT(id)
		FROM T_EQUIPMENT_BATTERY_PROBE
		WHERE 1=1
		<include refid="where-all-page-condition" />
	</select>

	<!-- query list without page -->
	<select id="selectEquipmenbatteryProbeListByCond" resultMap="equipmenbatteryProbeMapResult">
		SELECT
		<include refid="select-all-column" />
		FROM T_EQUIPMENT_BATTERY_PROBE WHERE 1=1
		<include refid="where-all-condition" />
	</select>

	<!-- query record -->
	<select id="selectEquipmenbatteryProbeByCond" resultMap="equipmenbatteryProbeMapResult">
		SELECT
		<include refid="select-all-column" />
		FROM T_EQUIPMENT_BATTERY_PROBE WHERE 1=1
		<include refid="where-all-condition" />
		LIMIT 1
	</select>

</mapper>