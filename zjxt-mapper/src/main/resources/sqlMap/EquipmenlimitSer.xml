<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sea.dao.EquipmenlimitSer">
	<resultMap id="equipmenlimitMapResult" type="com.ctevs.po.EquipmenlimitEntity">
		<result column="id" property="id" /> <!-- 序列 -->
		<result column="equipment_id" property="equipmentId" /> <!-- 设备ID 12位 -->
		<result column="batch_no" property="batchNo" /> <!-- 设备数据批次号 -->
		<result column="high_voltage_sys_no" property="highVoltageSysNo" /> <!-- 最高电压电池子系统号 -->
		<result column="high_voltage_one_battery_no" property="highVoltageOneBatteryNo" /> <!-- 最高电压电池单体代号 -->
		<result column="low_voltage_sys_no" property="lowVoltageSysNo" /> <!-- 最低电压电池子系统号 -->
		<result column="low_voltage_one_battery_no" property="lowVoltageOneBatteryNo" /> <!-- 最低电压电池单体代号 -->
		<result column="one_battery_voltage_high" property="oneBatteryVoltageHigh" /> <!-- 电池单体电压最高值 -->
		<result column="one_battery_voltage_low" property="oneBatteryVoltageLow" /> <!-- 电池单体电压最低值 -->
		<result column="high_temperature_sys_no" property="highTemperatureSysNo" /> <!-- 最高温度子系统号 -->
		<result column="high_temperature_probe_no" property="highTemperatureProbeNo" /> <!-- 最高温度探针序号 -->
		<result column="high_temperature" property="highTemperature" /> <!-- 最高温度值 -->
		<result column="low_temperature_sys_no" property="lowTemperatureSysNo" /> <!-- 最低温度子系统号 -->
		<result column="low_temperature_probe_no" property="lowTemperatureProbeNo" /> <!-- 最低温度探针序号 -->
		<result column="low_temperature" property="lowTemperature" /> <!-- 最低温度值 -->
		<result column="voltage_difference" property="voltageDifference" /> <!-- 压差值 -->
		<result column="temperature_difference" property="temperatureDifference" /> <!-- 温差值 -->
		<result column="collection_time" property="collectionTime" /> <!-- 信息采集时间 -->
		<result column="addtime" property="addtime" /> <!-- 创建时间 -->
	</resultMap>

	<sql id="select-all-column">
		id ,
		equipment_id ,
		batch_no ,
		high_voltage_sys_no ,
		high_voltage_one_battery_no ,
		low_voltage_sys_no ,
		low_voltage_one_battery_no ,
		one_battery_voltage_high ,
		one_battery_voltage_low ,
		high_temperature_sys_no ,
		high_temperature_probe_no ,
		high_temperature ,
		low_temperature_sys_no ,
		low_temperature_probe_no ,
		low_temperature ,
		voltage_difference ,
		temperature_difference ,
		collection_time ,
		addtime
	</sql>

	<sql id="where-all-page-condition">
		<if test="f.id != null">
			AND id = #{f.id}
		</if>
		<if test="f.equipmentId != null and f.equipmentId != ''">
			AND equipment_id = #{f.equipmentId}
		</if>
		<if test="f.batchNo != null">
			AND batch_no = #{f.batchNo}
		</if>
		<if test="f.highVoltageSysNo != null">
			AND high_voltage_sys_no = #{f.highVoltageSysNo}
		</if>
		<if test="f.highVoltageOneBatteryNo != null">
			AND high_voltage_one_battery_no = #{f.highVoltageOneBatteryNo}
		</if>
		<if test="f.lowVoltageSysNo != null">
			AND low_voltage_sys_no = #{f.lowVoltageSysNo}
		</if>
		<if test="f.lowVoltageOneBatteryNo != null">
			AND low_voltage_one_battery_no = #{f.lowVoltageOneBatteryNo}
		</if>
		<if test="f.oneBatteryVoltageHigh != null">
			AND one_battery_voltage_high = #{f.oneBatteryVoltageHigh}
		</if>
		<if test="f.oneBatteryVoltageLow != null">
			AND one_battery_voltage_low = #{f.oneBatteryVoltageLow}
		</if>
		<if test="f.highTemperatureSysNo != null">
			AND high_temperature_sys_no = #{f.highTemperatureSysNo}
		</if>
		<if test="f.highTemperatureProbeNo != null">
			AND high_temperature_probe_no = #{f.highTemperatureProbeNo}
		</if>
		<if test="f.highTemperature != null">
			AND high_temperature = #{f.highTemperature}
		</if>
		<if test="f.lowTemperatureSysNo != null">
			AND low_temperature_sys_no = #{f.lowTemperatureSysNo}
		</if>
		<if test="f.lowTemperatureProbeNo != null">
			AND low_temperature_probe_no = #{f.lowTemperatureProbeNo}
		</if>
		<if test="f.lowTemperature != null">
			AND low_temperature = #{f.lowTemperature}
		</if>
		<if test="f.voltageDifference != null">
			AND voltage_difference = #{f.voltageDifference}
		</if>
		<if test="f.temperatureDifference != null">
			AND temperature_difference = #{f.temperatureDifference}
		</if>
		<if test="f.collectionTime != null">
			AND collection_time = #{f.collectionTime}
		</if>
		<if test="f.addtime != null">
			AND addtime = #{f.addtime}
		</if>
	</sql>

	<sql id="where-all-condition">
		<if test="id != null">
			AND id = #{id}
		</if>
		<if test="equipmentId != null and equipmentId != ''">
			AND equipment_id = #{equipmentId}
		</if>
		<if test="batchNo != null">
			AND batch_no = #{batchNo}
		</if>
		<if test="highVoltageSysNo != null">
			AND high_voltage_sys_no = #{highVoltageSysNo}
		</if>
		<if test="highVoltageOneBatteryNo != null">
			AND high_voltage_one_battery_no = #{highVoltageOneBatteryNo}
		</if>
		<if test="lowVoltageSysNo != null">
			AND low_voltage_sys_no = #{lowVoltageSysNo}
		</if>
		<if test="lowVoltageOneBatteryNo != null">
			AND low_voltage_one_battery_no = #{lowVoltageOneBatteryNo}
		</if>
		<if test="oneBatteryVoltageHigh != null">
			AND one_battery_voltage_high = #{oneBatteryVoltageHigh}
		</if>
		<if test="oneBatteryVoltageLow != null">
			AND one_battery_voltage_low = #{oneBatteryVoltageLow}
		</if>
		<if test="highTemperatureSysNo != null">
			AND high_temperature_sys_no = #{highTemperatureSysNo}
		</if>
		<if test="highTemperatureProbeNo != null">
			AND high_temperature_probe_no = #{highTemperatureProbeNo}
		</if>
		<if test="highTemperature != null">
			AND high_temperature = #{highTemperature}
		</if>
		<if test="lowTemperatureSysNo != null">
			AND low_temperature_sys_no = #{lowTemperatureSysNo}
		</if>
		<if test="lowTemperatureProbeNo != null">
			AND low_temperature_probe_no = #{lowTemperatureProbeNo}
		</if>
		<if test="lowTemperature != null">
			AND low_temperature = #{lowTemperature}
		</if>
		<if test="voltageDifference != null">
			AND voltage_difference = #{voltageDifference}
		</if>
		<if test="temperatureDifference != null">
			AND temperature_difference = #{temperatureDifference}
		</if>
		<if test="collectionTime != null">
			AND collection_time = #{collectionTime}
		</if>
		<if test="addtime != null">
			AND addtime = #{addtime}
		</if>
	</sql>

	<!-- query list with page -->
	<select id="selectEquipmenlimitListByPageCond" resultMap="equipmenlimitMapResult">
		SELECT
		<include refid="select-all-column" />
		FROM T_EQUIPMENT_LIMIT WHERE 1=1
		<include refid="where-all-page-condition" />
		LIMIT
		#{beginCount},#{pageRows}
	</select>

	<!-- query list count -->
	<select id="selectEquipmenlimitListTotalCount" resultType="int">
		SELECT COUNT(id)
		FROM T_EQUIPMENT_LIMIT
		WHERE 1=1
		<include refid="where-all-page-condition" />
	</select>

	<!-- query list without page -->
	<select id="selectEquipmenlimitListByCond" resultMap="equipmenlimitMapResult">
		SELECT
		<include refid="select-all-column" />
		FROM T_EQUIPMENT_LIMIT WHERE 1=1
		<include refid="where-all-condition" />
	</select>

	<!-- query record -->
	<select id="selectEquipmenlimitByCond" resultMap="equipmenlimitMapResult">
		SELECT
		<include refid="select-all-column" />
		FROM T_EQUIPMENT_LIMIT WHERE 1=1
		<include refid="where-all-condition" />
		LIMIT 1
	</select>

</mapper>